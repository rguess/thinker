<%@ page contentType="text/html;charset=UTF-8" pageEncoding="UTF-8"%>

<h3 class="form-section">请假信息</h3>
<div class="span12">
	<div class="control-group">
		<label class="control-label">流程发起人：</label>
		<div class="controls">
			<span class="text" id="sponsor"></span>
		</div>
	</div>
</div>
<div class="span12">
	<div class="control-group">
		<label class="control-label">请假类型：</label>
		<div class="controls">
			<span class="text" id="leaveType"></span>
		</div>
	</div>
</div>
<div class="span12">
	<div class="control-group">
		<label class="control-label">开始日期：</label>
		<div class="controls">
			<span class="text" id="startTime"></span>
		</div>
	</div>
</div>
<div class="span12">
	<div class="control-group">
		<label class="control-label">结束日期：</label>
		<div class="controls">
			<span class="text" id="endTime"></span>
		</div>
	</div>
</div>
<div class="span12">
	<div class="control-group">
		<label class="control-label">请假天数：</label>
		<div class="controls">
			<span class="text" id="leaveDays"></span>
		</div>
	</div>
</div>

<div class="span12">
	<div class="control-group">
		<label class="control-label">文件：</label>
		<div class="controls">
			<span class="text" id="leavefile"></span>
		</div>
	</div>
</div>


<div class="span12">
	<div class="control-group">
		<label class="control-label">原因：</label>
		<div class="controls">
			<span class="text" id="leaveReason"></span>
		</div>
	</div>
</div>